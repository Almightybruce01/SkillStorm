<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Google AdMob — Required for AdMob SDK -->
	<key>GADApplicationIdentifier</key>
	<string>ca-app-pub-9418265198529416~1388107704</string>

	<!-- SKAdNetwork for Google -->
	<key>SKAdNetworkItems</key>
	<array>
		<dict>
			<key>SKAdNetworkIdentifier</key>
			<string>cstr6suwn9.skadnetwork</string>
		</dict>
	</array>

	<!-- App Transport Security — Allow ad network calls -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSAllowsArbitraryLoadsForMedia</key>
		<true/>
		<key>NSAllowsArbitraryLoadsInWebContent</key>
		<true/>
	</dict>

	<!-- Export Compliance — App uses only exempt encryption (HTTPS/TLS) -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>

	<!-- Privacy -->
	<key>NSPrivacyPolicyURL</key>
	<string>https://skillzstorm.com/privacy</string>
</dict>
</plist>
